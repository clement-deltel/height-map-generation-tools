<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane maxHeight="100.0" maxWidth="600.0" minHeight="100.0" minWidth="600.0" prefHeight="100.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminAddView">
   <center>
      <HBox prefHeight="120.0" prefWidth="360.0" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="License :">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="8.0" />
               </HBox.margin>
            </Text>
            <TextField fx:id="admin_add_textfield_license" promptText="XXXX-XXXX-XXXX-XXXX">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <MenuButton fx:id="admin_add_menu_button_type" maxWidth="130.0" minWidth="130.0" mnemonicParsing="false" prefWidth="130.0" text="License Type">
              <items>
                <MenuItem mnemonicParsing="false" text="User" />
                <MenuItem mnemonicParsing="false" text="Admin" />
              </items>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </MenuButton>
            <MenuButton fx:id="admin_add_menu_button_duration" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" prefWidth="100.0" text="Duration">
               <items>
                  <MenuItem mnemonicParsing="false" text="1 Day" />
                  <MenuItem mnemonicParsing="false" text="7 Day" />
                  <MenuItem mnemonicParsing="false" text="31 Day" />
                  <MenuItem mnemonicParsing="false" text="365 Day" />
               </items>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </MenuButton>
            <Button maxWidth="60.0" minWidth="60.0" mnemonicParsing="false" onAction="#adminAddLicense" prefWidth="60.0" text="ADD">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </center>
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="Add License :" BorderPane.alignment="CENTER_LEFT">
         <BorderPane.margin>
            <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
         </BorderPane.margin>
         <font>
            <Font size="14.0" />
         </font>
      </Text>
   </top>
   <bottom>
      <Text fx:id="admin_text_message" fill="#c72020" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" BorderPane.alignment="CENTER_LEFT">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </Text>
   </bottom>
</BorderPane>
